{
  "name": "Croptop",
  "description": "Publish and access indie content freely through .eth domains",
  "author": "Jango",
  "authorWebsite": "https://croptop.eth.sucks",
  "version": "1.1.20",
  "idealItemsPerPage": 10,
  "generateIndexPagination": false,
  "buildNumber": 582,
  "generateNFTMetadata": true,
  "generateTagPages": true,
  "settings": {
    "collectionAddress": {
      "name": "Collection Address",
      "type": "string",
      "defaultValue": "",
      "description": "The Ethereum contract address of the collection."
    },
    "collectionCategory": {
      "name": "Collection Category",
      "type": "int",
      "defaultValue": "0",
      "description": "The collection's category where posts will be recorded."
    },
    "curatorAddress": {
      "name": "Curator Address",
      "type": "string",
      "defaultValue": "",
      "description": "The Ethereum address of the curator of this collection."
    },
    "ethereumMainnetRPC": {
      "name": "Ethereum Mainnet RPC",
      "type": "string",
      "defaultValue": "https://eth.llamarpc.com",
      "description": ""
    },
    "ethereumSepoliaRPC": {
      "name": "Ethereum Sepolia RPC",
      "type": "string",
      "defaultValue": "https://ethereum-sepolia-rpc.publicnode.com",
      "description": ""
    },
    "optimismMainnetRPC": {
      "name": "Optimism Mainnet RPC",
      "type": "string",
      "defaultValue": "https://optimism.llamarpc.com",
      "description": ""
    },
    "optimismSepoliaRPC": {
      "name": "Optimism Sepolia RPC",
      "type": "string",
      "defaultValue": "https://sepolia.optimism.io",
      "description": ""
    },
    "baseMainnetRPC": {
      "name": "Base Mainnet RPC",
      "type": "string",
      "defaultValue": "https://base.llamarpc.com",
      "description": ""
    },
    "baseSepoliaRPC": {
      "name": "Base Sepolia RPC",
      "type": "string",
      "defaultValue": "https://sepolia.base.org",
      "description": ""
    },
    "arbitrumMainnetRPC": {
      "name": "Arbitrum Mainnet RPC",
      "type": "string",
      "defaultValue": "https://arbitrum.llamarpc.com",
      "description": ""
    },
    "arbitrumSepoliaRPC": {
      "name": "Arbitrum Sepolia RPC",
      "type": "string",
      "defaultValue": "https://sepolia-rollup.arbitrum.io/rpc",
      "description": ""
    },
    "highlightColor": {
      "name": "Highlight Color",
      "type": "string",
      "defaultValue": "#F056C1",
      "description": "Color for links and buttons"
    }
  }
}
